<div class="span0 well text-center">
  <% if logged_in? %>
    <%= link_to vote_path(obj, true), method: 'post' do %>
      <i class="icon-arrow-up"></i>
    <% end %>
    <p><%= pluralize obj.total_votes, 'vote' %> </p>
    <%= link_to vote_path(obj, false), method: 'post' do %>
      <i class="icon-arrow-down"></i>
    <% end %>
  <% else %>
    <i class="icon-arrow-up"></i>
    <p><%= obj.total_votes %> Votes</p>
    <i class="icon-arrow-down"></i>
  <% end %>
</div>
